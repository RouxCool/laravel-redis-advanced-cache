{
    "name": "rouxcool/laravel-redis-advanced-cache",
    "description": "Advanced Redis-based caching system for Laravel REST and Orion controllers.",
    "type": "library",
    "license": "MIT",
    "version": "1.2.4",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "RedisAdvancedCache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedisAdvancedCache\\Providers\\RedisCacheServiceProvider"
            ],
            "aliases": {
                "RedisCache": "RedisAdvancedCache\\Facades\\RedisCache"
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0"
    }
}
